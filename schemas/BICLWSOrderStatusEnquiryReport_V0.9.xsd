							</xs:sequence>
							<xs:element name="ProductIdentifier" type="bicws:ProductIdentifier"
								maxOccurs="unbounded"/>
						</xs:choice>
						<xs:element name="ItemDescription" minOccurs="0">
							<xs:complexType>
