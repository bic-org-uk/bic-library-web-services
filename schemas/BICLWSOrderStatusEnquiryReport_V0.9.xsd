					<xs:sequence>
						<xs:element name="ReferenceTypeCode"
							type="bicws:OrderStatusEnquiryItemReferenceTypeCode"/>
						<xs:choice>
							<xs:sequence>
								<xs:element name="ReferenceNumber" type="xs:string"/>
								<xs:element name="ReferenceDateTime" type="bicws:DateOrDateTime"
									minOccurs="0"/>
